   "source": [
    "canvas_size = 100\n",
    "square_size = 10\n",
    "\n",
    "num_squares = canvas_size // square_size\n",
    "\n",
    "for row in range(num_squares):\n",
    "    for col in range(num_squares):\n",
    "        html = f'<div class=\"square square-row-{row} square-col-{col}\" id=\"square-{row}-{col}\"></div>'\n",
    "        print (html)\n",
    "\n",
    "    print ()\n",
    "\n",
    "\n"
   ]
